{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "incidents",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "incident-status-tracking",
      "collectionName": "incidents",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDBConnectionString"
    }
  ],
  "scriptFile": "../dist/CreateIncident/index.js"
}
